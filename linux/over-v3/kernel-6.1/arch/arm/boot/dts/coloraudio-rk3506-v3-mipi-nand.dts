&sai2 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <>;
};

&sai3 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&sai3_lrck_pins &sai3_sclk_pins &sai3_sdo_pins>;
};

&saradc {
	vref-supply = <&vcc_1v8>;
	status = "okay";
};

&tsadc {
	status = "okay";
};

&u2phy_otg0 {
	vbus-supply = <&vcc5v0_otg0>;
	rockchip,vbus-always-on;
	status = "okay";
};

&u2phy_otg1 {
	phy-supply = <&vcc5v0_otg1>;
	status = "okay";
};

&usb2phy {
	status = "okay";
};

&usb20_otg0 {
	status = "okay";
};

&usb20_otg1 {
	dr_mode = "host";
	status = "okay";
};

&display_subsystem {
	logo-memory-region = <&drm_logo>;
	status = "okay";
};

&pwm0_4ch_0 {
	pinctrl-names = "active";
	pinctrl-0 = <&rm_io21_pwm0_ch0>;
	status = "okay";
};

&pwm0_4ch_2 {
	pinctrl-names = "active";
	pinctrl-0 = <&rm_io4_pwm0_ch2>;
	status = "okay";
};

&dsi {
	status = "okay";
	// rockchip,lane-rate = <850>;
	dsi_panel: panel@0 {
		status = "okay";
		compatible = "simple-panel-dsi";
		reset-gpios  = <&gpio1 RK_PA2 GPIO_ACTIVE_LOW>;
		pinctrl-names = "default";
		pinctrl-0 = <&lcd_reset_gpio>;
		reg = <0>;
		backlight = <&backlight>;

		reset-delay-ms = <30>;
		enable-delay-ms = <100>;
		prepare-delay-ms = <20>;
		unprepare-delay-ms = <20>;
		disable-delay-ms = <20>;
		init-delay-ms = <120>;

		width-mm = <68>;
		height-mm = <121>;

		dsi,flags = <(MIPI_DSI_MODE_VIDEO | MIPI_DSI_MODE_VIDEO_BURST |
			      MIPI_DSI_MODE_LPM | MIPI_DSI_MODE_NO_EOT_PACKET)>;
		dsi,format = <MIPI_DSI_FMT_RGB888>;
		dsi,lanes  = <2>;
		panel-init-sequence = [
			05 78 01 01
			05 78 01 11
			39 00 06 FF 77 01 00 00 11
			15 00 02 D1 11
			15 00 02 55 B0
			39 00 06 FF 77 01 00 00 10
			39 00 03 C0 63 00
			39 00 03 C1 09 02
			39 00 03 C2 37 08
			15 00 02 C7 00 
			15 00 02 CC 38
			39 00 11 B0 00 11 19 0C 10 06 07 0A 09 22 04 10 0E 28 30 1C
			39 00 11 B1 00 12 19 0D 10 04 06 07 08 23 04 12 11 28 30 1C
			39 00 06 FF 77 01 00 00 11
			15 00 02 B0 4D
			15 00 02 B1 60 
			15 00 02 B2 07
			15 00 02 B3 80
			15 00 02 B5 47
			15 00 02 B7 8A
			15 00 02 B8 21
			15 00 02 C1 78
			15 00 02 C2 78
			15 64 02 D0 88
			39 00 04 E0 00 00 02
			39 00 0C E1 01 A0 03 A0 02 A0 04 A0 00 44 44
			39 00 0D E2 00 00 00 00 00 00 00 00 00 00 00 00
			39 00 05 E3 00 00 33 33
			39 00 03 E4 44 44
			39 00 11 E5 01 26 A0 A0 03 28 A0 A0 05 2A A0 A0 07 2C A0 A0
			39 00 05 E6 00 00 33 33
			39 00 03 E7 44 44
			39 00 11 E8 02 26 A0 A0 04 28 A0 A0 06 2A A0 A0 08 2C A0 A0
			39 00 08 EB 00 01 E4 E4 44 00 40
			39 00 11 ED FF F7 65 4F 0B A1 CF FF FF FC 1A B0 F4 56 7F FF
			39 00 06 FF 77 01 00 00 00
			15 00 02 36 00
			15 00 02 3A 77
			05 78 01 11
			05 14 01 29
		];

		// 15 00 02 3A 55 16bit

		panel-exit-sequence = [
			05 00 01 28
			05 00 01 10
		];

		disp_timings0: display-timings {
			native-mode = <&dsi_timing0>;
			dsi_timing0: timing0 {
				clock-frequency = <27000000>;
				hactive = <480>;       //与 LCDTiming.LCDH 对应
				vactive = <800>;       //与 LCDTiming.LCDV 对应
				hfront-porch = <32>;   //与 LCDTiming.HFPD 对应
				hsync-len = <4>;       //与 LCDTiming.HSPW 对应
				hback-porch = <32>;    //与 LCDTiming.HBPD 对应
				vfront-porch = <9>;    //与 LCDTiming.VEPD 对应
				vsync-len = <4>;       //与 LCDTiming.VsPW 对应
				vback-porch = <3>;     //与 LCDTiming.VBPD 对应
				hsync-active = <0>;
				vsync-active = <0>;
				de-active = <0>;
				pixelclk-active = <0>;
			};
		};

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;
				panel_in_dsi: endpoint {
					remote-endpoint = <&dsi_out_panel>;
				};
			};
		};
	};

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@1 {
			reg = <1>;
			dsi_out_panel: endpoint {
				remote-endpoint = <&panel_in_dsi>;
			};
		};
	};
};

&dsi_panel {
	power-supply = <&vcc3v3_lcd_n>;
};

&dsi_dphy {
    status = "okay";
};

&dsi_in {
	status = "okay";
};

&dsi_in_vop {
	status = "okay";
};

&route_dsi{
	status = "okay";
	connect = <&vop_out_dsi>;
};

&vop {
	status = "okay";
};